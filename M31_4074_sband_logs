#Delay Calibration:

gaincal(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning_secondcal.K0', 
        field='0',refant='ea04',spw='0~15:21~43',gaintype='K',solint='inf',
combine='scan',minsnr=5,gaintable=['M31_4074_Sband_hanning.antpos','M31_4074_Sband_hanning_secondcal.initPh'])

