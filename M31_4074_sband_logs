#initial calibration: 

gencal(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning.antpos',caltype='antpos')

#Initial Phase Calibration 

gaincal(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning_intitial_phase.initPh',field='0,1,8',refant='ea04',
spw='0~15:21~43',gaintype='G',calmode='p',solint='30s',minsnr=5,gaintable=['M31_4074_Sband_hanning.antpos'])

plotms(vis='M31_4074_Sband_hanning_intitial_phase.initPh',xaxis='time',yaxis='phase',coloraxis='corr',iteraxis='antenna',plotrange=[-1,-1,-180,180])

gaincal(vis='M31_4074_Sband_hanning.ms', caltable='M31_4074_Sband_hanning.initPh', 
        field='0', refant='ea04', spw='0~15:21~43', calmode='p', solint='30s', 
        minsnr=5, gaintable=['M31_4074_Sband_hanning.antpos'])



plotms(vis='M31_4074_Sband_hanning.initPh',xaxis='time',yaxis='phase',coloraxis='corr',field='0',iteraxis='antenna',plotrange=[-1,-1,-180,180])


#Delay Calibration

gaincal(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning.K0', 
        field='0',refant='ea04',spw='0~15:21~43',gaintype='K',solint='30s',
combine='scan',minsnr=5,gaintable=['M31_4074_Sband_hanning.antpos','M31_4074_Sband_hanning.initPh'])

plotms(vis='M31_4074_Sband_hanning.K0',xaxis='antenna1',yaxis='delay',coloraxis='baseline')

#BandPass Calibration

bandpass(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning.B0',
         field='0',spw='',refant='ea04',combine='scan', 
         solint='30s',bandtype='B',
         gaintable=['M31_4074_Sband_hanning.antpos',
                    'M31_4074_Sband_hanning.initPh',
                    'M31_4074_Sband_hanning.K0'])

plotms(vis='M31_4074_Sband_hanning.B0',field='0', 
        xaxis='chan',yaxis='amp',coloraxis='corr',
        iteraxis='antenna',gridrows=2,gridcols=2)

plotms(vis='M31_4074_Sband_hanning.B0',field='0', 
        xaxis='chan',yaxis='phase',coloraxis='corr',plotrange=[-1,-1,-180,180],
        iteraxis='antenna',gridrows=2,gridcols=2)

gaincal(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning.G1',
        field='0',spw='0~15:21~43',
        solint='30s',refant='ea04',gaintype='G',calmode='ap',solnorm=False,
        gaintable=['M31_4074_Sband_hanning.antpos',
                    'M31_4074_Sband_hanning.initPh',
                   'M31_4074_Sband_hanning.K0',
                   'M31_4074_Sband_hanning.B0'],
        interp=['linear','linear','linear','nearest'])


gaincal(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning.G1',
        field='1',spw='0~15:21~43',
        solint='30s',refant='ea04',gaintype='G',calmode='ap',solnorm=False,
        gaintable=['M31_4074_Sband_hanning.antpos',
                    'M31_4074_Sband_hanning.initPh',
                   'M31_4074_Sband_hanning.K0',
                   'M31_4074_Sband_hanning.B0'],append=True)


applycal(vis='M31_4074_Sband_hanning.ms',
         field='0,1,8',
         gaintable=['M31_4074_Sband_hanning.antpos',
                     'M31_4074_Sband_hanning.G1' ,
                    'M31_4074_Sband_hanning.initPh',
                    'M31_4074_Sband_hanning.K0',
                    'M31_4074_Sband_hanning.B0'],
         gainfield=['','','','',''], 
         interp=['linear','linear','linear','linear','nearest'],
         calwt=False)


plotms(vis='M31_4074_Sband_hanning.ms',field='0',correlation='RR,LL',
       antenna='',avgtime='',xaxis='channel',yaxis='amp',
       ydatacolumn='corrected',coloraxis='corr',
       plotfile='plotms_M31-fld0-corrected-amp.png')




