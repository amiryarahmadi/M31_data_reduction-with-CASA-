        xaxis='chan',yaxis='amp',coloraxis='corr',
        iteraxis='antenna',gridrows=2,gridcols=2)


plotms(vis='M31_4074_Sband_hanning_secondcal.B0',field='0', 
        xaxis='chan',yaxis='phase',coloraxis='corr',plotrange=[-1,-1,-180,180],
        iteraxis='antenna',gridrows=2,gridcols=2)



gaincal(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning_secondcal.G1',
        field='0',spw='0~15:21~43',
        solint='inf',refant='ea04',gaintype='G',calmode='ap',solnorm=False,
