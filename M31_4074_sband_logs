#Bandpass

bandpass(vis='M31_4074_Sband_hanning.ms',caltable='M31_4074_Sband_hanning_secondcal.B0',
         field='0',spw='',refant='ea04',combine='scan', 
         solint='inf',bandtype='B',
         gaintable=['M31_4074_Sband_hanning.antpos',
                    'M31_4074_Sband_hanning_secondcal.initPh',
                    'M31_4074_Sband_hanning_secondcal.K0'])

